    "homepage": "https://www.freecadweb.org",
    "license": "LGPL-2.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/FreeCAD/FreeCAD/LatestRelease/FreeCAD-0.19.3-WIN-x64-portable.7z",
            "hash": "8b7ca7385f269e3253ef82408330190654cee46a0ee6bb42c391a560cb01393c"
        }
    },
    "pre_install": [
        "$baseDir=(Get-Item $dir\\*)[0]",
        "Get-Item $dir\\* | Get-ChildItem | Move-Item -Destination $dir",
        "Remove-Item $baseDir"
    ],
    "bin": "bin\\FreeCADCmd.exe",
    "shortcuts": [
        [
            "bin\\FreeCAD.exe",
            "FreeCAD"
        ]
    ],
    "checkver": {
        "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/FreeCAD/FreeCAD/LatestRelease/",
        "regex": "FreeCAD-(?<build>[\\w.]+)-WIN"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/FreeCAD/FreeCAD/LatestRelease/FreeCAD-$matchBuild-WIN-x64-portable1.7z"
            }
        },
        "hash": {
            "url": "$url-SHA256.txt",
            "regex": "SHA256(?sm):\\s+([a-fA-F\\d\\s]+)"
        }
    }
